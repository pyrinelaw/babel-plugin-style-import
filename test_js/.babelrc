{
  "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                }
            }
        ],
        "es2015",
        "stage-2"
    ],

    "plugins": ["syntax-dynamic-import", "add-module-exports", "transform-runtime"]
}
